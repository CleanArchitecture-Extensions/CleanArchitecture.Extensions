{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Extensions": {
    "Core": {
      "CorrelationHeaderName": "X-Correlation-ID",
      "EnablePerformanceLogging": true,
      "PerformanceWarningThreshold": "00:00:00.400",
      "GuardStrategy": "ReturnFailure"
    }
  },
  "AllowedHosts": "*"
}
